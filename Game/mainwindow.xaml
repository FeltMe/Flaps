<Window x:Class="Game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="450" ResizeMode="NoResize">

    <Window.Background>
        <ImageBrush ImageSource= "Pictures/Niceee.png">
           
        </ImageBrush>
    </Window.Background>

    <Grid>
        <Button Content="START" HorizontalAlignment="Left" Margin="102,103,0,0" VerticalAlignment="Top" Width="266" Height="74" FontFamily="Wide Latin" FontSize="36" Click="Button_Click"/>
       
        <Button Content="LEVELS" HorizontalAlignment="Left" Margin="102,198,0,0" VerticalAlignment="Top" Width="266" Height="48" FontFamily="Wide Latin" FontSize="24" Click="Button_Click_1"/>
        <Button Content="HELP" HorizontalAlignment="Left" Margin="102,273,0,0" VerticalAlignment="Top" Width="129" RenderTransformOrigin="0.68,0.128" Height="38" FontFamily="Wide Latin" FontSize="20" Click="Button_Click_2"/>
        <Button Content="EXIT" HorizontalAlignment="Left" Margin="264,273,0,0" VerticalAlignment="Top" Width="104" Height="38" FontFamily="Wide Latin" FontSize="20"/>
    </Grid>
</Window>
