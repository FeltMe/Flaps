        <Button Content="4" HorizontalAlignment="Left" Margin="119,225,0,0" VerticalAlignment="Top" Width="65" Height="65" RenderTransformOrigin="-3.216,2.541" FontFamily="Wide Latin" FontSize="48" Click="Button_Click_4"/>
        <Button Content="5" HorizontalAlignment="Left" Margin="304,225,0,0" VerticalAlignment="Top" Width="65" Height="65" FontFamily="Wide Latin" FontSize="48" Click="Button_Click_5"/>
        <Button Content="6" HorizontalAlignment="Left" Margin="489,225,0,0" VerticalAlignment="Top" Width="65" Height="65" FontFamily="Wide Latin" FontSize="48" Click="Button_Click_6"/>
